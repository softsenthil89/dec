pipeline{
    agent any
    stages
    {
        stage('build')
        {
            steps{
                echo "Build is running"
            }
        }
        stage('test')
        {
            steps{
                echo "Test is running"
            }
        }
        stage('deploy')
        {
            steps{
                touch ssfile
                echo "Deploy is running"
            }
        }
    }
}
